{
    "name": "florenceegi/hub",
    "description": "EGI-HUB - Central Control Application for the entire FlorenceEGI ecosystem",
    "type": "project",
    "license": "proprietary",
    "keywords": ["florenceegi", "hub", "multi-tenant", "aggregations", "natan", "egi"],
    "homepage": "https://florenceegi.com",
    "authors": [
        {
            "name": "Fabio Cherici",
            "email": "fabio@florenceegi.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "FlorenceEgi\\Hub\\": "src/"
        }
    },
    "scripts": {
        "dev": "./start.sh",
        "start": "./start.sh",
        "backend": "cd backend && php artisan serve --port=8001",
        "frontend": "cd frontend && npm run dev"
    },
    "extra": {
        "laravel": {
            "providers": [
                "FlorenceEgi\\Hub\\HubServiceProvider"
            ]
        }
    }
}
